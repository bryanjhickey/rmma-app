{
  "name": "renegade-web",
  "description": "Gatsby website for The Renegade Academy",
  "version": "1.0.0",
  "private": true,
  "author": "Bryan Hickey <bryanjhickey@gmail.com> (https://bryanjhickey.com.au)",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "starter",
    "typescript"
  ],
  "scripts": {
    "start": "export NODE_OPTIONS=--openssl-legacy-provider && gatsby develop",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && gatsby build",
    "serve": "export NODE_OPTIONS=--openssl-legacy-provider && gatsby serve",
    "clean": "export NODE_OPTIONS=--openssl-legacy-provider && gatsby clean",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "test": "jest",
    "commit": "cz",
    "preload-fonts": "gatsby-preload-fonts"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@react-icons/all-files": "4.1.0",
    "@sanity/image-url": "1.0.1",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.4.0",
    "@tailwindcss/line-clamp": "0.3.1",
    "axios": "0.25.0",
    "date-fns": "2.28.0",
    "dotenv": "15.0.0",
    "framer-motion": "6.2.3",
    "gatsby": "4.6.0",
    "gatsby-node-helpers": "1.2.1",
    "gatsby-plugin-config": "1.0.7",
    "gatsby-plugin-image": "2.6.0",
    "gatsby-plugin-manifest": "4.6.0",
    "gatsby-plugin-preload-fonts": "3.6.0",
    "gatsby-plugin-react-helmet": "5.6.0",
    "gatsby-plugin-remove-console": "0.0.2",
    "gatsby-plugin-sharp": "4.6.0",
    "gatsby-source-filesystem": "4.6.0",
    "gatsby-source-google-places": "1.0.1",
    "gatsby-source-sanity": "7.3.2",
    "gatsby-transformer-sharp": "4.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-icons": "4.3.1",
    "react-intersection-observer": "8.33.1",
    "tsconfig-paths-webpack-plugin": "3.5.2"
  },
  "devDependencies": {
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@netlify/plugin-lighthouse": "2.1.3",
    "@tailwindcss/typography": "0.5.1",
    "@testing-library/dom": "8.11.3",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.4.0",
    "@types/node": "16.11.21",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-helmet": "6.1.5",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "autoprefixer": "10.4.2",
    "babel-jest": "27.4.6",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-preset-gatsby": "2.6.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.8.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.0.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "gatsby-plugin-postcss": "5.6.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.4.7",
    "lint-staged": "12.3.2",
    "postcss": "8.4.6",
    "prettier": "2.5.1",
    "prettier-plugin-tailwindcss": "0.1.4",
    "tailwindcss": "3.0.18",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
